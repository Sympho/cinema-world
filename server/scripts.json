{
  "allow": {
    "env": true,
    "run": true,
    "net": true,
    "read": "server",
    "write": "server",
    "plugin": true
  },
  "scripts": {
    "start": {
      "cmd": "deno run  --unstable server/index.ts",
      "desc": "run my index.ts file"
    }
  },
  "watcher": {
    "match": ["server/**/*.*"]
  }
}
